                                        <input type="text" class="form-control" name="SUB_ZONE_NAME" id="SUB_ZONE_NAME" />
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    @Resources.SettingsE.ResourceManager.GetString("SUB_ZONE_NAME" + "_" + ViewBag.lang)
