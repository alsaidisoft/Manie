        <div class="card-body">

            <div class="col-lg-12">
                @if (Language.GetCurrentLang() == "en")
                {
                    <h1 class="text-center font-weight-bolder text-primary" style="font-family: Arial;">Choose pass type that you want in  (@ViewBag.Station)</h1>
