@using APP.Util;
@model IEnumerable<SecurityClearanceWebApp.Models.RA42_WORKFLOW_RESPONSIBLE_MST>

@{
    ViewBag.Title = "Index";
    string align = "text-right";
    string alignInverse = "text-left";
    string dir = "ltr";
    var table = "data-table1";
    var active = "active";
    var nactive = "not active";

    if (ViewBag.lang == "ar")
    {
        align = "text-left";
        alignInverse = "text-right";
        dir = "rtl";
        table = "data-table";
        active = "مفعل";
        nactive = "محضور";

    }

}

<style>
    .select2-container {
        width: 100% !important;
    }
</style>


<div class="container-fluid" dir="@dir">
    <div class="row">
        @{
            Html.RenderPartial("settings_nav_links");

        }
        <div class="col-md-10">
            <div class="card shadow-sm">
                <div class="card-body">
                    <div class="row">
                        <div class="col-6 justify-content-center text-primary">
                            <h4>
                                <i class="@ViewBag.controllerIconClass"></i>&nbsp;
                                @Resources.Settings.ResourceManager.GetString("WORKFLOW_TYPE_Permissions" + "_" + ViewBag.lang)
                            </h4>
                        </div>
                        <div class="col-6 text-white @align">
                            <a class="btn btn-success" href="#" onclick="AddNew(0)">
                                <i class="fa fa-plus-circle"></i>
                                &nbsp;
                                @Resources.Common.ResourceManager.GetString("create" + "_" + ViewBag.lang)
                            </a>
                        </div>
                    </div>
                </div>
                <div class="card-body">
                    @if (TempData["Erorr"] != null)
                    {
                        <div class="alert alert-danger text-center p-1">@TempData["Erorr"]</div>
                    }
                    <table class="table table-striped table-bordered table-hover table-responsive @table  text-center" style="display: table">
                        <thead class="bg-primary text-white">
                            <tr>
                                <th>#</th>
                                @if (ViewBag.DEVELOPER == true)
                                {
                                    <th>
                                        @Resources.Settings.ResourceManager.GetString("Force" + "_" + ViewBag.lang)
                                    </th>
                                }
                                @if (ViewBag.DEVELOPER == true || ViewBag.ADMIN == true)
                                {
                                    <th>
                                        @Resources.Settings.ResourceManager.GetString("UNIT_CODE" + "_" + ViewBag.lang)
                                    </th>
                                }
                                <th>
                                    @Resources.Settings.ResourceManager.GetString("sect" + "_" + ViewBag.lang)
                                </th>
                                <th>
                                    @Resources.Settings.ResourceManager.GetString("SERVICE_NUMBER" + "_" + ViewBag.lang)
                                </th>
                                <th>
                                    @Resources.Settings.ResourceManager.GetString("Respo_name" + "_" + ViewBag.lang)
                                </th>
                                <th>
                                    @Resources.Settings.ResourceManager.GetString("Passestypes" + "_" + ViewBag.lang)
                                </th>
                                <th>
                                    @Resources.Settings.ResourceManager.GetString("Step_name" + "_" + ViewBag.lang)
                                </th>
                                <th>
                                    @Resources.Settings.ResourceManager.GetString("Active" + "_" + ViewBag.lang)
                                </th>

                                <th style="border-right: 1px solid #ccc;">
                                    @Resources.Settings.ResourceManager.GetString("ACTIONS" + "_" + ViewBag.lang)
                                </th>

                            </tr>
                        </thead>
                        <tbody>
                            @{int rowNo = 0;}
                            @foreach (var item in Model)
                            {
                            <tr>
                                @{rowNo++;}
                                <td>@rowNo</td>


                                @if (ViewBag.DEVELOPER == true)
                                {
                                    <td>
                                        @if (Language.GetCurrentLang() == "en")
                                        {
                                            @Html.DisplayFor(modelItem => item.RA42_STATIONS_MST.RA42_FORCES_MST.FORCE_NAME_E)

                                        }
                                        else
                                        {
                                            @Html.DisplayFor(modelItem => item.RA42_STATIONS_MST.RA42_FORCES_MST.FORCE_NAME_A)

                                        }

                                    </td>
                                }
                                @if (ViewBag.DEVELOPER == true || ViewBag.ADMIN == true)
                                {
                            <td width="15%">
                                @if (Language.GetCurrentLang() == "en")
                                {
                                    @Html.DisplayFor(modelItem => item.RA42_STATIONS_MST.STATION_NAME_E)

                                }
                                else
                                {
                                    @Html.DisplayFor(modelItem => item.RA42_STATIONS_MST.STATION_NAME_A)

                                }
                            </td>
                                }
                            <td>
                                @if (Language.GetCurrentLang() == "en")
                                {
                                    @item.UNIT_NAME_E
                                }
                                else
                                {
                                    @item.UNIT_NAME
                                }
                            </td>
                                <td>@item.SERVICE_NUMBER</td>
                                <td>
                                    @{
                                        var slash = " - ";
                                    }
                                    @if (Language.GetCurrentLang() == "en")
                                    {
                                        @item.RANK_E @slash @item.RESPONSEPLE_NAME_E
                                    }
                                    else
                                    {
                                        @item.RANK @slash @item.RESPONSEPLE_NAME
                                    }
                                </td>
                                <td>
                                    @if (Language.GetCurrentLang() == "en")
                                    {
                                        foreach (var x in item.RA42_ACCESS_SELECT_MST.Where(a => a.DLT_STS != true))
                                        {
                                            @Html.DisplayFor(model => x.RA42_ACCESS_TYPE_MST.ACCESS_TYPE_E)
                                            string _underscor = "-";
                                            @_underscor
                                        }
                                    }
                                    else
                                    {
                                        foreach (var x in item.RA42_ACCESS_SELECT_MST.Where(a => a.DLT_STS != true))
                                        {
                                            @Html.DisplayFor(model => x.RA42_ACCESS_TYPE_MST.ACCESS_TYPE)
                                            string _underscor = "-";
                                            @_underscor
                                        }
                                    }
                                </td>
                                <td width="15%">
                                    @if (Language.GetCurrentLang() == "en")
                                    {
                                        @Html.DisplayFor(modelItem => item.RA42_WORKFLOW_MST.STEP_NAME_E)

                                    }
                                    else
                                    {
                                        @Html.DisplayFor(modelItem => item.RA42_WORKFLOW_MST.STEP_NAME)

                                    }

                                </td>
                                <td width="15%">
                                    @if (item.ACTIVE != false)
                                    {

                                    <div class="alert alert-success rounded text-center">
                                        <i class="fa fa-check-circle"></i> @Resources.Settings.ResourceManager.GetString("Activated" + "_" + ViewBag.lang)
                                    </div>
                                    }
                                    else
                                    {
                                <div class="alert alert-danger rounded text-center">
                                    <i class="fa fa-times-circle"></i> @Resources.Settings.ResourceManager.GetString("NotActivated" + "_" + ViewBag.lang)
                                </div>
                                    }
                                    </td>




                                <td style="border-right: 1px solid #ccc;">
                                    <div class="btn-group" dir="ltr">
                                        <a class="btn btn-primary details btn-sm" href="#" onclick="Details(@item.WORKFLOW_RESPO_CODE)">
                                            <i class="fa fa-eye"></i>
                                        </a>
                                        <a class="btn btn-warning btn-sm" href="#" onclick="EditRecord(@item.WORKFLOW_RESPO_CODE)">
                                            <i class="fa fa-edit"></i>
                                        </a>

                                        <a class="btn btn-danger btn-sm" href="#" onclick="DeleteRecord(@item.WORKFLOW_RESPO_CODE)">
                                            <i class="fa fa-trash"></i>
                                        </a>

                                    </div>
                                </td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@*Create A Popup Modal With Registration Form For Add Or Edit Student Record*@

<div class="modal fade" id="MyModal" dir="@dir">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">

            <div class="modal-header">
                <div class="col-md-12">
