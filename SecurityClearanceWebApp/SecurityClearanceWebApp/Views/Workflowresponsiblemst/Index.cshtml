                                <input type="text" class="form-control" name="UNIT_NAME" id="UNIT_NAME_AR" />
                            </div>
                        </div>
                        }
                        <div class="col-md-12">
                            <div class="row">
                                <div class="col-md-6" id="access1">
                                    <div class="form-group">
                                        <div class="control-label">
                                            @Resources.Settings.ResourceManager.GetString("Passestypes" + "_" + ViewBag.lang)
                                        </div>
                                        <select class="form-control select2-selector" name="ACCESS_TYPE_CODE[]" multiple style="width:100%;" id="ACCESS_TYPE_CODE1">
                                            @foreach (var ltfo in (MultiSelectList)ViewBag.ACCESS_TYPE_CODE)
                                            {

                                                <option value="@ltfo.Value">@ltfo.Text</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-6" id="access2">
                                    <div class="form-group">
                                        <div class="control-label">
                                            @Resources.Settings.ResourceManager.GetString("Passestypes" + "_" + ViewBag.lang)
                                        </div>
                                        <select class="form-control select2-selector" name="ACCESS_TYPE_CODE[]" multiple style="width:100%;" id="ACCESS_TYPE_CODE2">

                                            @foreach (var ltfo in (MultiSelectList)ViewBag.ACCESS_TYPE_CODE)
                                            {
                                                <option value="@ltfo.Value">@ltfo.Text</option>
                                            }

                                        </select>



                                    </div>
                                </div>

                                <div class="col-md-6" id="wo1">
                                    <div class="form-group">
                                        <div class="control-label">
                                            @Resources.Settings.ResourceManager.GetString("WORKFLOW_TYPE" + "_" + ViewBag.lang)
                                        </div>
                                        <select class="form-control" name="WORKFLOWID" id="WORKFLOWID1">
                                            @foreach (var ltfo in ViewBag.WORKFLOWID)
                                            {
                                                <option value="@ltfo.Value" @if (ltfo.Value == "12") { <text> selected</text>} @*@if ((ViewBag.workflowIDType != 4 && ltfo.Value == "4") && (ViewBag.ADMIN !=true && ViewBag.DEVELOPER !=true)) {<text>disabled="disabled"</text>}*@>@ltfo.Text</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-6" id="wo2">
                                    <div class="form-group">
                                        <div class="control-label">
                                            @Resources.Settings.ResourceManager.GetString("WORKFLOW_TYPE" + "_" + ViewBag.lang)
                                        </div>
                                        <select class="form-control" name="WORKFLOWID" id="WORKFLOWID2">
                                            @foreach (var ltfo in ViewBag.WORKFLOWID)
                                            {
                                                <option value="@ltfo.Value" @*@if ((ViewBag.workflowIDType != 4 && ltfo.Value == "4") && (ViewBag.ADMIN !=true && ViewBag.DEVELOPER !=true)) {<text>disabled="disabled"</text>}*@>@ltfo.Text</option>
                                            }
                                        </select>
                                    </div>
                                </div>

                                <div class="col-md-6" id="uni1">
                                    <div class="form-group">
                                        <div class="control-label">
                                            @Resources.Settings.ResourceManager.GetString("UNIT_CODE" + "_" + ViewBag.lang)
                                        </div>
                                        <select class="form-control" name="STATION_CODE" id="UNIT_CODE1">
                                            @foreach (var ltfo in ViewBag.STATION_CODE)
                                            {
                                                <option value="@ltfo.Value">@ltfo.Text</option>
                                            }
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-6" id="uni2">
                                    <div class="form-group">
                                        <div class="control-label">
                                            @Resources.Settings.ResourceManager.GetString("UNIT_CODE" + "_" + ViewBag.lang)
                                        </div>
                                        <select class="form-control" name="STATION_CODE" id="UNIT_CODE2">
                                            @foreach (var ltfo in ViewBag.STATION_CODE)
                                            {
                                                <option value="@ltfo.Value">@ltfo.Text</option>
                                            }
                                        </select>
                                    </div>
                                </div>

                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="card">
                                <div class="card-header">
                                    @Resources.Settings.ResourceManager.GetString("Choose_permission" + "_" + ViewBag.lang)
                                </div>

                                <div class="row p-2">

                                    @if (ViewBag.ADMIN == true || ViewBag.DEVELOPER == true || ViewBag.RP == true)
                                    {
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                @Resources.Settings.ResourceManager.GetString("Rp" + "_" + ViewBag.lang)
                                                <input type="checkbox" class="form-check-input m-1" name="RP" value="true" id="RP" />

                                            </div>
                                        </div>
                                    }
                                    @if (ViewBag.ADMIN == true || ViewBag.DEVELOPER == true || ViewBag.STN == true)
                                    {
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                @Resources.Settings.ResourceManager.GetString("Settings_pr" + "_" + ViewBag.lang)
                                                <input type="checkbox" class="form-check-input m-1" name="SETTINGS" value="true" id="SETTINGS" />

                                            </div>
                                        </div>
                                    }

                                    @if (ViewBag.DEVELOPER == true)
                                    {
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                @Resources.Settings.ResourceManager.GetString("Developer" + "_" + ViewBag.lang)
                                                <input type="checkbox" class="form-check-input m-1" name="DEVELOPER" value="true" id="DEVELOPER" />
                                            </div>
                                        </div>
                                    }
                                    @if (ViewBag.ADMIN == true || ViewBag.DEVELOPER == true)
                                    {
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                @Resources.Settings.ResourceManager.GetString("All_permissions" + "_" + ViewBag.lang)
                                                <input type="checkbox" class="form-check-input m-1" name="ADMIN" value="true" id="ADMIN" />
                                            </div>
                                        </div>
                                    }

                                </div>
                            </div>
                            <br />
                        </div>
                        <div class="col-md-12">
                            <div class="form-group">
                                @Resources.Settings.ResourceManager.GetString("Active" + "_" + ViewBag.lang)
                                <input type="checkbox" class="form-check-input m-1" name="ACTIVE" value="false" id="boxid1"><label for="boxid1" class="text-success font-weight-bold text-left w-50" id="b1">مفعل</label>
                                <input type="checkbox" class="form-check-input m-1" name="ACTIVE" value="false" id="boxid2"><label for="boxid2" class="text-success font-weight-bold text-left w-25 pr-4" id="b2"></label>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <a href="#" class="btn btn-block text-center" id="SaveRecord"></a>

                            </div>
                        </div>
                    </fieldset>
                </form>
            </div>
        </div>
    </div>
</div>

