                                <input type="text" class="form-control" name="RESPONSEPLE_NAME" id="RESPONSEPLE_NAME" />
                            </div>
                            <hr />

                        </div>
                        @{
                            int se = 0;
                        }
                        @foreach (var i in ViewBag.SECTION_NAME)
                        {
                            se = se + 1;
                        }

                        @if (se != 0)
                        {

                            <div class="col-md-12">
                                @Resources.Settings.ResourceManager.GetString("sect" + "_" + ViewBag.lang)
