                                        {
                                            <option value="@item.Value">@item.Text</option>
                                        }



                                    </select>
                                </div>
                            </div>
                        }
                        else
                        {
                      <div class="col-md-12">
                            @Resources.Settings.ResourceManager.GetString("sect" + "_" + ViewBag.lang)
