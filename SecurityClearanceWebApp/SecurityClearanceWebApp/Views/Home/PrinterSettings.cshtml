@using APP.Util


@{
    ViewBag.Title = "Index";
    string align = "text-right";
    string alignInverse = "text-left";
    string dir = "ltr";
    if (ViewBag.lang == "ar")
    {
        align = "text-left";
        alignInverse = "text-right";
        dir = "rtl";
    }


}
<div class="container-fluid" dir="@dir">
    <div class="card shadow-lg">





        <h1 class="card-header text-primary"><i class="fa fa-video"></i> @Resources.Settings.ResourceManager.GetString("printer_settings" + "_" + ViewBag.lang)</h1>
        <div class="card-body text-center">

            <div class="col-md-12 justify-content-center">
                <div class="row">
                    <div class="col-md-6 text-center">
                        <h3 class="text-center text-secondary"> @Resources.Common.ResourceManager.GetString("fargo" + "_" + ViewBag.lang)</h3>
                        @if (ViewBag.workflowIDType == 3 || ViewBag.ADMIN == true || ViewBag.DEVELOPER == true)
                        {
                            <video style="width:90%; height:60% !important; margin: 0 auto; padding:4px;" controls>
                                <source src="~/Content/videos/printsettings.mp4" type="video/mp4" />
                                <source src="~/Content/videos/printsettings.mp4" type="video/ogg" />
                                Your Browser Dose Not Support the Video Tag
                            </video>
                        }
                    </div>
                    <div class="col-md-6 text-center">
                        <h3 class="text-center text-secondary">@Resources.Common.ResourceManager.GetString("datacard" + "_" + ViewBag.lang)</h3>

                        @if (ViewBag.workflowIDType == 3 || ViewBag.ADMIN == true || ViewBag.DEVELOPER == true)
                        {
                            <video style="width:90%; height:60% !important;margin: 0 auto; padding:4px;" controls>
                                <source src="~/Content/videos/datacard_settings.mp4" type="video/mp4" />
                                <source src="~/Content/videos/datacard_settings.mp4" type="video/ogg" />
                                Your Browser Dose Not Support the Video Tag
                            </video>
                        }
                    </div>

                    <div class="col-md-6 text-center">
                        <h3 class="text-center text-secondary">@Resources.Common.ResourceManager.GetString("rao_fargo" + "_" + ViewBag.lang)</h3>

                        @if (ViewBag.workflowIDType == 3 || ViewBag.ADMIN == true || ViewBag.DEVELOPER == true)
                        {
                            <video style="width:90%; height:60% !important;margin: 0 auto; padding:4px;" controls>
                                <source src="~/Content/videos/fargo_rao.mp4" type="video/mp4" />
                                <source src="~/Content/videos/fargo_rao.mp4" type="video/ogg" />
                                Your Browser Dose Not Support the Video Tag
                            </video>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>

<br />

